[package]
name = "open-erase-web"
version = "0.1.0"
edition = "2024"
build = "build.rs"

[dependencies]
# local dependencies
open-erase-lib = { path = "../lib", features = ["schemas"] }

# public dependencies
console_error_panic_hook = "0.1.7"
icondata = "0.6.0"
leptos = { version = "0.8.12", features = ["csr"] }
leptos_icons = "0.7.0"
leptos_router = { version = "0.8.9", features = ["tracing"] }
reqwest = { version = "0.22.1", features = ["json"] }
wasm-bindgen = "=0.2.100"
